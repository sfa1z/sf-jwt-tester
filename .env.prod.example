# Salesforce Connected App Client ID (also known as Consumer Key)
CLIENT_ID="3MVG9..."

# Salesforce username for authentication
USERNAME="your.username@your-domain.com"

# Path to your Java keystore file (.jks)
KEYSTORE_PATH="your-org-id.jks"

# Password for the keystore
KEYSTORE_PASSWORD="your-keystore-password"

# Salesforce login URL for Production
# Use https://login.salesforce.com for production
LOGIN_URL="https://login.salesforce.com"

# JWT Audience - should match LOGIN_URL
JWT_AUDIENCE="https://login.salesforce.com"
